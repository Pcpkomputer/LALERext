{
	"manifest_version": 2,

	"name": "LALERext",
	"version": "1.0",
	"description": "Serikat Anti Laler",

	"background": {
		"scripts": ["background.js"],
		 "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.3.min.js", "app.js"]
    }
  ],
  "permissions": ["<all_urls>"]
}
